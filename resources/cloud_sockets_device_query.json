{
  "$schema" : "http://json-schema.org/draft-07/schema#",
  "type" : "object",
  "properties" : {
    "error" : {
      "type" : "integer",
      "description": "Error code"
    },
    "apikey" : {
      "type" : "string",
      "description": "User apikey"
    },
    "deviceid" : {
      "type" : "string",
      "description": "Device ID"
    },
    "params" : {
      "type" : "object",
      "description": "Parameters which will be passed transparently. The server will not verify this filed."
    }
  },
  "required" : [
    "error",
    "apikey",
    "deviceid",
    "params"
  ]
}
