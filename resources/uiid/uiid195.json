{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "description":"UIID107 NS Panel pro" ,
  "type": "object",
  "properties": {
    "fwVersion": {
      "oneOf": [
        {
          "type": "null"
        },
        {
          "type": "string"
        }
      ],
      "description": "The current firmware version number, e.g. 3.4.0",
      "default": null
    },
    "temperature": {
      "oneOf": [
        {
          "type": "null"
        },
        {
          "type": "number"
        }
      ],
      "default": null
    }
  }
}
