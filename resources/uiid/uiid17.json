{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "description": "UIID17 Three-Speed Smart Fan",
  "type": "object",
  "properties": {
    "sledOnline": {
      "oneOf": [
        {
          "type": "null"
        },
        {
          "type": "string",
          "enum": ["on", "off"]
        }
      ],
      "description": "Network Indicator Switch,turn on (on), turn off (off)",
      "default": null
    },
    "fwVersion": {
      "oneOf": [
        {
          "type": "null"
        },
        {
          "type": "string"
        }
      ],
      "description": "The current firmware version number, e.g. 3.4.0",
      "default": null
    },
    "chipid": {
      "oneOf": [
        {
          "type": "null"
        },
        {
          "type": "string"
        }
      ],
      "description": "Chip id, generally considered to be a fixed parameter of a chip. Each chip is unique",
      "default": null
    },
    "ssid": {
      "oneOf": [
        {
          "type": "null"
        },
        {
          "type": "string"
        }
      ],
      "default": null
    },
    "rssi": {
      "oneOf": [
        {
          "type": "null"
        },
        {
          "type": "integer"
        }
      ],
      "description": "WiFi signal strength, Unit: dBm,e.g. -55",
      "default": null
    },
    "fan": {
      "oneOf": [
        {
          "type": "null"
        },
        {
          "type": "string",
          "enum": ["on", "off"]
        }
      ],
      "description": "Switch of Fan, turn on (on), turn off (off)",
      "default": null
    },
    "speed": {
      "oneOf": [
        {
          "type": "null"
        },
        {
          "type": "string",
          "enum": ["slow", "moderate", "fast"]
        }
      ],
      "description": "Speed, slow speed (slow); moderate speed (moderate); fast speed (fast)",
      "default": null
    },
    "mode": {
      "oneOf": [
        {
          "type": "null"
        },
        {
          "type": "string",
          "enum": ["normal", "natural", "sleep"]
        }
      ],
      "description": "Mode, normal mode (normal); natural mode (natural) ; sleep mode (sleep)",
      "default": null
    },
    "shake": {
      "oneOf": [
        {
          "type": "null"
        },
        {
          "type": "string",
          "enum": ["on", "off"]
        }
      ],
      "description": "Shake, turn on (on), turn off (off)",
      "default": null
    }
  }
}
