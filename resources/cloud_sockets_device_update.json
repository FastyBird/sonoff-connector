      "description": "Error code"
    },
    "apikey" : {
      "type" : "string",
      "description": "User apikey"
    },
    "deviceid" : {
      "type" : "string",
      "description": "Device ID"
    },
    "sequence" : {
      "type" : "string",
      "description": "Timestamp accurate to milliseconds"
    },
    "params" : {
      "type" : "object",
